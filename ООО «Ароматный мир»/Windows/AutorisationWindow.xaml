<Window x:Class="ООО__Ароматный_мир_.Windows.AutorisationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ООО__Ароматный_мир_.Windows"
        mc:Ignorable="d"
        Title="Автроризация" Height="448" Width="218" MinHeight="448" MinWidth="218">
    <Grid>
        <StackPanel VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
            <Image Source="/Resorces/LogoImage/logo.png" MaxHeight="250" MaxWidth="250" ></Image>
            <TextBlock>Логин</TextBlock>
            <TextBox x:Name="LoginText" Background="#FFFFCC99"></TextBox>
            <TextBlock>Пароль</TextBlock>
            <PasswordBox x:Name="PasswordText"></PasswordBox>
            <Button Click="Button_Click" Background="#FFCC6600">Войти</Button>
            <Button Click="Button_Click_1">Войти как гость</Button>
        </StackPanel>
    </Grid>
</Window>
